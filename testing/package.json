{
  "name": "vote.gov-cypress-testing-suite",
  "version": "1.0.0",
  "description": "## Requirements",
  "main": "index.js",
  "devDependencies": {
    "axe-core": "^4.6.3",
    "cypress": "^12.3.0",
    "cypress-axe": "^1.4.0",
    "cypress-mochawesome-reporter": "^3.3.0"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:test": "cypress run --spec 'cypress/e2e/tests/**/*'",
    "cy:axe": "cypress run --spec 'cypress/e2e/accessibility/**/*'",
    "cy:links": "cypress run --spec 'cypress/e2e/internalLinks/**/*'",
    "cy:proofer": "cypress run --spec 'cypress/e2e/externalLinks/**/*'",
    "cy:backEnd": "cypress run --spec 'cypress/e2e/backend/**/*'",
    "testSuite": "npm run cy:test && npm run cy:links && npm run cy:axe",
    "cypress": "cypress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usagov/vote-gov-drupal.git"
  },
