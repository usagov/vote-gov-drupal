{#
/**
 * @file
 * Default theme implementation to display a block content: Basic.
 */
#}

{# Caching the data in content object #}
{% set savedCache = content | render %}

{% set field_classes = content.field_classes | field_value | render %}

<div{{ attributes.addClass(field_classes) }}>
  {# Hold these title_* placeholders for module integrations #}
  {{ title_prefix }}
  {{ title_suffix }}
  {% if content.field_heading | render %}
    <h2 class="vote-heading--red-underline">{{ content.field_heading | field_value }}</h2>
  {% endif %}
  {{ content.body | field_value }}
</div>
