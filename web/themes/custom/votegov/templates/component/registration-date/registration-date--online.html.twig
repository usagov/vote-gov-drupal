{#
/**
 * partial
 * Voter online registration deadline
 *
 */
#}
{% set deadline = content.field_online_deadline | field_value | default(content.field_g_online_deadline | field_value) %}

{% if deadline %}
  <li><strong>{{ 'Online registration deadline:' | t }}</strong> {{ deadline }}</li>
{% endif %}

<script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "FAQPage",
    "mainEntity": [{
      "@type": "Question",
      "name": "{{ 'How to register and update your registration' | t }}",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "{{ '@link on @state_name’s election website.' | t({
    '@link': { "#markup": "<a href=\"#{registration_link}\" data-state-ext=\"online-registration\">#{registration_link_text}</a>" } | render,
    '@state_name': state_name,
    '@of_state_name': of_state_name,
    '@in_state_name': in_state_name
  }) }}"
    }
    },{
        "@type": "Question",
        "name": "{{ 'Other ways to register to vote' | t }}",
        "acceptedAnswer": {
          "@type": "Answer",
          "text": "{{ 'You can also @link on @state_name’s election website.' | t({
    '@link': { "#markup": "<a href=\"#{more_info_link}\" data-state-ext=\"offline-registration\">#{more_info_link_text}</a>" } | render,
    '@state_name': state_name,
    '@of_state_name': of_state_name,
    '@in_state_name': in_state_name
  }) }}"
    }
    },{
        "@type": "Question",
        "name": "{{ 'Voter registration deadlines' | t }}",
        "acceptedAnswer": {
          "@type": "Answer",
          "text": "{{ 'Register by mail deadline:' | t }} {{ deadline }}"
        }
      }{{ end }}]
  }
</script>
