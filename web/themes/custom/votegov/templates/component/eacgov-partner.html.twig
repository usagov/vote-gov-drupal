{% set GSAlink = link('General Services Administration' | t, 'https://www.gsa.gov/') %}

<div class="vote-eac-partner vote-block-padding-y-narrow" data-test="footerContact">
  <div class="vote-eac-partner__container">
    <div class="vote-eac-partner__logo">
      {# EAC Text #}
      <div class="vote-eac-partner__text">
        <p>{{ 'Vote.govÂ is in partnership with' | t }}</p>
      </div>
      {# EAC logo #}
      <div class="vote-eac-partner__logo-img">
        <a extlinkjs-ignore
           href="{{ language == 'es' ? 'https://www.eac.gov/translations/spanish' : 'https://www.eac.gov/' }}"
           target="_blank"
           class="vote-eac-partner__logo-link">
          <img width="64" height="64" src="{{ theme_path }}/img/eac-logo.png"
