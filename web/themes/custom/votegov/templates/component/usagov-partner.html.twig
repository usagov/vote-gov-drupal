{% if partner %}
<div class="vote-usagov-partner">
  {# USA.gov Links #}
  <nav class="vote-usagov-partner__links" aria-label="{{ partner.field_section_identifier | field_value }}">
    {# USA.gov election info links #}
    <h2 class="vote-heading--red-underline">{{ partner.field_heading | field_value }}</h2>
    {% if partner.field_election_resources | render %}
      <ul>
        {% for resource in partner.field_election_resources | field_value %}
          <li>{{ resource }}</li>
        {% endfor %}
      </ul>
    {% endif %}
  </nav>
  {# Row 2 Divider - USA.gov link and email sign-up #}
  <div class="vote-usagov-partner__contact-container">
    <div class="vote-usagov-partner__contact">
      <div class="vote-usagov-partner__logo">
        {# USA.gov link #}
        <a href="{{ partner.field_usagov_logo_link | field_value }}" extlinkjs-ignore>
          {{ partner.field_usagov_logo | field_value }}
        </a>
        <div class="vote-usagov-partner__disclaimer">
          {{ partner.field_usagov_logo_text | field_value }}
        </div>
      </div>
      {# Email sign-up component. #}
      {{ drupal_entity('block_content', 4) }}
    </div>
  </div>
</div>
{% endif %}
